{
  "name": "rm-poc",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run local-client-start-watch",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test-watch": "nodemon ./dist-server/server/temp/temp.js",
    "local-client-start-watch": "ng serve --host localhost --sourcemaps true --hmr -e=hmr",
    "client-start-watch": "ng serve --host rm --sourcemaps true --hmr -e=hmr",
    "client-test-watch": "ng test --sourcemaps=true --colors=true",
    "server-test-watch": "jasmine-node ./dist-server/server --autotest --watch ./dist-server/server --color",
    "server-test-watch-verbose": "jasmine-node ./dist-server/server --autotest --watch ./dist-server/server --color --verbose",
    "server-compile-watch": "tsc --watch -p . --outdir ./dist-server",
    "server-start-watch": "nodemon ./dist-server/server/server.js",
    "server-query-start-watch": "nodemon ./dist-server/server/rm-demo/query-microservice.js",
    "server-command-start-watch": "nodemon ./dist-server/server/rm-demo/command-microservice.js",
    "graphql-start-watch": "nodemon ./dist-server/server/graphql/graphql-server.js",
    "hmr": "ng serve --host 0.0.0.0 --sourcemaps true --hmr -e=hmr"
  },
